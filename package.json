{
  "name": "simpletimeclock",
  "version": "1.0.0",
  "description": "A simple time clock to keep track of your hours.",
  "main": "src/main/app.js",
  "scripts": {
    "start": "electron .",
    "sass": "node-sass src/renderer/public/scss/style.scss src/renderer/public/css/style.css",
    "sass:w": "node-sass src/renderer/public/scss/style.scss src/renderer/public/css/style.css -wo",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Simple Time Clock\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Andrew Lee",
  "license": "MIT",
  "dependencies": {
    "cleave.js": "^1.4.6",
    "electron": "^3.0.7",
    "exceljs": "^1.6.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.2",
    "electron-packager": "^13.0.1"
  }
}
